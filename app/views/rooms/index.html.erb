<p id="notice"><%= notice %></p>
<div class="row">
  <div class="col-md-6">
    <h1>Rooms</h1>

    <ul class="mt-4 list-unstyled">
      <%= render @rooms %>
    </ul>

    <div class="mt-2">
      <%= render 'rooms/form', room: @room %>
    </div>
  </div>
  <div class="col-md-6">
    <h1>Users online</h1>

    <p class="mt-4" id="online"><% @users.each do |u| %>
      <%= "#{u} " %>
    <% end %></p>
  </div>
</div>

